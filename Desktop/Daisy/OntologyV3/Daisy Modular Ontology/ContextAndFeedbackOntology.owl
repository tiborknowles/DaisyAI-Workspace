<?xml version='1.0' encoding='utf-8'?>
<rdf:RDF xmlns:ns1="http://www.w3.org/2002/07/owl#" xmlns:ns2="http://www.w3.org/2000/01/rdf-schema#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <ns1:Ontology rdf:about="http://www.autonomousrecordlabel.com/ontology#ContextAndFeedbackOntology">
    <ns2:label>ContextAndFeedback Ontology</ns2:label>
    <ns2:comment>Ontology module for ContextAndFeedback concepts.</ns2:comment>
    <ns1:versionInfo>0.1.0</ns1:versionInfo>
    <ns1:imports rdf:resource="DaisyCoreOntology.owl" />
    <!-- ADDED: Missing import for CognitiveTask -->
    <ns1:imports rdf:resource="AgentCognitiveFrameworkOntology.owl" />
  </ns1:Ontology>
  
  <ns1:Class rdf:about="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor">
    <ns2:label xml:lang="en">MCP Contextual Factor</ns2:label>
    <ns2:comment xml:lang="en">Represents a piece of contextual information derived from MCP (Multi-Agent Contextual Processing) layers or other external data sources that can influence decisions or KPI outcomes.</ns2:comment>
    <ns2:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing" />
  </ns1:Class>
  
  <ns1:Class rdf:about="http://www.autonomousrecordlabel.com/ontology#AgentFeedback">
    <ns2:subClassOf rdf:resource="http://www.autonomousrecordlabel.com/ontology#InteractionArtifact" />
    <ns2:label xml:lang="en">Agent Feedback</ns2:label>
    <ns2:comment xml:lang="en">Feedback provided by an AI agent about its own task execution, challenges, observations, or the state of its environment.</ns2:comment>
  </ns1:Class>
  
  <ns1:Class rdf:about="http://www.autonomousrecordlabel.com/ontology#HumanFeedback">
    <ns2:subClassOf rdf:resource="http://www.autonomousrecordlabel.com/ontology#InteractionArtifact" />
    <ns2:label xml:lang="en">Human Feedback</ns2:label>
    <ns2:comment xml:lang="en">Feedback provided by a human user on an agent's recommendation, action, simulation outcome, or other system aspect.</ns2:comment>
  </ns1:Class>
  
  <ns1:Class rdf:about="http://www.autonomousrecordlabel.com/ontology#Recommendation">
    <ns2:subClassOf rdf:resource="http://www.autonomousrecordlabel.com/ontology#InteractionArtifact" />
    <ns2:label xml:lang="en">Recommendation</ns2:label>
    <ns2:comment xml:lang="en">A suggestion or proposal, typically for an action or decision, generated by an AI agent or a human. It should be supported by evidence and context.</ns2:comment>
  </ns1:Class>
  
  <ns1:Class rdf:about="http://www.autonomousrecordlabel.com/ontology#KnowledgeSourceConfidence">
    <ns2:label xml:lang="en">Knowledge Source Confidence</ns2:label>
    <ns2:comment xml:lang="en">Represents the assessed confidence in a piece of information, a data source (like an MCP feed), or a derived insight.</ns2:comment>
    <ns2:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing" />
  </ns1:Class>
  
  <!-- Datatype Properties -->
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#factorDescription">
    <ns2:label xml:lang="en">factor description</ns2:label>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#factorSource">
    <ns2:label xml:lang="en">factor source</ns2:label>
    <ns2:comment xml:lang="en">Identifies the origin of the contextual factor (e.g., 'SocialMediaSentimentFeed', 'StreamingPlatformTrendAPI', 'MCP_TopicModelOutput').</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#factorTimestamp">
    <ns2:label xml:lang="en">factor timestamp</ns2:label>
    <ns2:comment xml:lang="en">Timestamp when the contextual factor was observed or generated.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#associatedRawDataSnippet">
    <ns2:label xml:lang="en">associated raw data snippet</ns2:label>
    <ns2:comment xml:lang="en">Optional raw data or a summary associated with the contextual factor.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#feedbackContent">
    <ns2:label xml:lang="en">feedback content</ns2:label>
    <ns2:domain>
      <ns1:Class>
        <ns1:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.autonomousrecordlabel.com/ontology#AgentFeedback" />
          <rdf:Description rdf:about="http://www.autonomousrecordlabel.com/ontology#HumanFeedback" />
        </ns1:unionOf>
      </ns1:Class>
    </ns2:domain>
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#isAcknowledgedByMaestro">
    <ns2:label xml:lang="en">is acknowledged by Maestro</ns2:label>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#AgentFeedback" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#humanSentiment">
    <ns2:label xml:lang="en">human sentiment</ns2:label>
    <ns2:comment xml:lang="en">Sentiment expressed in human feedback (e.g., Positive, Neutral, Negative).</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#HumanFeedback" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#recommendationText">
    <ns2:label xml:lang="en">recommendation text</ns2:label>
    <ns2:comment xml:lang="en">The textual content or summary of the recommendation.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#recommendationConfidence">
    <ns2:label xml:lang="en">recommendation confidence</ns2:label>
    <ns2:comment xml:lang="en">The confidence score associated with the recommendation.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#confidenceValue">
    <ns2:label xml:lang="en">confidence value</ns2:label>
    <ns2:comment xml:lang="en">A numerical value (e.g., 0.0 to 1.0) representing the confidence.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#KnowledgeSourceConfidence" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal" />
  </ns1:DatatypeProperty>
  
  <ns1:DatatypeProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#confidenceDerivationMethod">
    <ns2:label xml:lang="en">confidence derivation method</ns2:label>
    <ns2:comment xml:lang="en">How the confidence value was derived (e.g., 'ExpertElicitation', 'StatisticalAnalysis', 'SensorAccuracySpec').</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#KnowledgeSourceConfidence" />
    <ns2:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
  </ns1:DatatypeProperty>
  
  <!-- Object Properties -->
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#potentialImpactOnKPI">
    <ns2:label xml:lang="en">potential impact on KPI</ns2:label>
    <ns2:comment xml:lang="en">Links a contextual factor to a KPI it might influence.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#KPI" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#relevanceToGoal">
    <ns2:label xml:lang="en">relevance to goal</ns2:label>
    <ns2:comment xml:lang="en">Links a contextual factor to a Goal it is relevant for.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#Goal" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#feedbackFromAgent">
    <ns2:label xml:lang="en">feedback from agent</ns2:label>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#AgentFeedback" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#AIAgent" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#relatedToAction">
    <ns2:label xml:lang="en">related to action</ns2:label>
    <ns2:comment xml:lang="en">Links feedback to a specific AgentAction it pertains to.</ns2:comment>
    <ns2:domain>
      <ns1:Class>
        <ns1:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.autonomousrecordlabel.com/ontology#AgentFeedback" />
          <rdf:Description rdf:about="http://www.autonomousrecordlabel.com/ontology#HumanFeedback" />
        </ns1:unionOf>
      </ns1:Class>
    </ns2:domain>
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#AgentAction" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#suggestsImprovementTo">
    <ns2:label xml:lang="en">suggests improvement to</ns2:label>
    <ns2:comment xml:lang="en">Agent feedback might suggest improvements to a task type, tool, or process.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#AgentFeedback" />
    <ns2:range>
      <ns1:Class>
        <ns1:unionOf rdf:parseType="Collection">
          <rdf:Description rdf:about="http://www.autonomousrecordlabel.com/ontology#CognitiveTask" />
          <rdf:Description rdf:about="http://www.autonomousrecordlabel.com/ontology#Tool" />
        </ns1:unionOf>
      </ns1:Class>
    </ns2:range>
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#feedbackFromHuman">
    <ns2:label xml:lang="en">feedback from human</ns2:label>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#HumanFeedback" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#Human" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#relatedToRecommendation">
    <ns2:label xml:lang="en">related to recommendation</ns2:label>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#HumanFeedback" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#relatedToSimulation">
    <ns2:label xml:lang="en">related to simulation</ns2:label>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#HumanFeedback" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#SimulationRun" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#recommendsAction">
    <ns2:label xml:lang="en">recommends action</ns2:label>
    <ns2:comment xml:lang="en">The specific AgentAction being recommended.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#AgentAction" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#evidenceForRecommendation">
    <ns2:label xml:lang="en">evidence for recommendation</ns2:label>
    <ns2:comment xml:lang="en">Links the recommendation to supporting evidence, which could be data, other facts, or contextual factors.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
    <ns2:range rdf:resource="http://www.w3.org/2002/07/owl#Thing" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#incorporatesContextualFactor">
    <ns2:label xml:lang="en">incorporates contextual factor</ns2:label>
    <ns2:comment xml:lang="en">Links a recommendation to specific MCPContextualFactors that informed it.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#MCPContextualFactor" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#considersAgentFeedback">
    <ns2:label xml:lang="en">considers agent feedback</ns2:label>
    <ns2:comment xml:lang="en">Links a recommendation to AgentFeedback that was considered in its formulation.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#Recommendation" />
    <ns2:range rdf:resource="http://www.autonomousrecordlabel.com/ontology#AgentFeedback" />
  </ns1:ObjectProperty>
  
  <ns1:ObjectProperty rdf:about="http://www.autonomousrecordlabel.com/ontology#appliesToInformation">
    <ns2:label xml:lang="en">applies to information</ns2:label>
    <ns2:comment xml:lang="en">Links a confidence assessment to the specific piece of information or data source it pertains to.</ns2:comment>
    <ns2:domain rdf:resource="http://www.autonomousrecordlabel.com/ontology#KnowledgeSourceConfidence" />
    <ns2:range rdf:resource="http://www.w3.org/2002/07/owl#Thing" />
  </ns1:ObjectProperty>
</rdf:RDF>